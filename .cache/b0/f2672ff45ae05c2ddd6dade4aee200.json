columns: 1fr;\r\n    grid-auto-rows: minmax(150px, auto);\r\n    grid-template-rows: minmax(300px, auto);\r\n    grid-template-areas: \r\n    \"header\"\r\n    \"main\"\r\n    \"footer\";\r\n}\r\n@media screen and (min-width: 730px) {\r\n    \r\n    .container {\r\n        grid-template-columns: repeat(3, 1fr);\r\n        // grid-template-rows: 800px minmax(150px, auto) 100px;\r\n        grid-template-areas: \r\n        \"header header header \"\r\n        \"main main main\"\r\n        \"footer footer footer\"; \r\n        .header {\r\n            height: 90vh;\r\n            .content {\r\n                margin-top: 250px;\r\n            }  \r\n        }\r\n        .main {\r\n            display: flex;\r\n            flex-direction: row;\r\n            justify-content: center;\r\n        }\r\n    }\r\n\r\n}\r\n","scss\\_navbar.scss":"@use \"./mixins\";\r\n@use \"./variables\";\r\n\r\n/* ===================================== \r\n        ------Navigation------\r\n======================================== */\r\n        \r\n.navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    font-size: 24px;\r\n    margin: 10px 5px ;\r\nh2{\r\n    color: variables.$h2Color;\r\n    text-shadow: 2px 2px variables.$siteThemeColor ;\r\n}\r\n    .loginBtn {\r\n        @include mixins.btnStyles();  \r\n    }\r\n}\r\n/* ===================================== \r\n        ------/Navigation------\r\n======================================== */","scss\\_variables.scss":"$h2Color: white;\r\n$siteThemeColor: rgb(129, 102, 224);","scss\\_mixins.scss":"@use \"./variables\";\r\n@mixin btnStyles () {\r\n    border: none;\r\n    border-radius: 16px;\r\n    padding: 6px 10px;\r\n    background-color: variables.$siteThemeColor;\r\n    text-decoration: none;\r\n    color: white;\r\n    font-size: 20px;\r\n    font-family: 'Montserrat', sans-serif;\r\n}","scss\\_maincards.scss":"@use \"./variables\";\r\n@use \"./mixins\";\r\n/* ===================================== \r\n        ------Main------\r\n======================================== */\r\n.main {\r\n    display: flex;\r\n    flex-direction: column;\r\n    grid-area: main;\r\n    background-color: rgb(238, 237, 237);\r\n    padding: 1rem;\r\n    .card {\r\n        background-color: white;\r\n        padding: 20px;\r\n        border: 1px solid #e1e1e1;\r\n        border-bottom: 8px solid gray;\r\n        border-radius: 6px;\r\n        margin: 10px;\r\n        a {\r\n            color: #278da4;\r\n            text-decoration: none;\r\n        }\r\n        p {\r\n            margin-bottom: 20px;\r\n            padding: 50px;\r\n            \r\n        }\r\n        h1 {\r\n            color: variables.$siteThemeColor; \r\n            margin: 5px 0 0;\r\n            line-height: 40px;\r\n        }\r\n        .btn {\r\n            @include mixins.btnStyles();\r\n        }\r\n    }\r\n}\r\n/* ===================================== \r\n        ------/Main------\r\n======================================== */","scss\\_header.scss":"@use \"./maincards\";\r\n@use \"./mixins\";\r\n@use \"./variables\";\r\n\r\n/* ===================================== \r\n    ------Header------\r\n======================================== */\r\n.header {\r\n    background-image: url(\"../img/movies.jpg\");\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    grid-area: header;\r\n    .content{\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        margin-top: 60px;\r\n        .header-h2 {\r\n            color: variables.$h2Color;\r\n            font-weight: 900;\r\n            text-shadow: 2px 3px rgb(20, 20, 20) ;      \r\n        }\r\n        .headerInput {\r\n            border: none;\r\n            padding: 10px;\r\n            margin: 20px;\r\n            width: 430px     \r\n        }\r\n        .headerBtn{\r\n            @include mixins.btnStyles();\r\n            width: 430px;  \r\n        }\r\n    }\r\n}\r\n/* ===================================== \r\n    -----/Header------\r\n======================================== */","scss\\_footer.scss":"@use \"./maincards\";\r\n@use \"./mixins\";\r\n@use \"./variables\";\r\n/* ===================================== \r\n        ------Footer------\r\n======================================== */\r\nfooter {\r\n    display: flex;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    grid-area: footer;\r\n    margin-top: 20px;\r\n\r\n.footerBtn {\r\n    @include mixins.btnStyles();\r\n    margin-top: 40px;\r\n}\r\nq {\r\n    color: black;\r\n    font-size: 20px;\r\n}\r\n}\r\n\r\n/* ===================================== \r\n        ------/Footer------\r\n======================================== */"},"lineCount":null}},"error":null,"hash":"168b765a9f4bdc25017f4e96c567fb08","cacheData":{"env":{}}}